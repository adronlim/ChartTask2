{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "DynamiChart": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/DynamiChart",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "src/assets/Template/vendor/datatables/dataTables.bootstrap4.min.css",
              "src/assets/Template/css/sb-admin.min.css",
              "src/assets/Template/scss/sb-admin.scss"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/popper.js/dist/umd/popper.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.min.js",
              "src/assets/Template/vendor/jquery/jquery.min.js",
              "src/assets/Template/vendor/jquery-easing/jquery.easing.min.js",
              "src/assets/Template/vendor/datatables/dataTables.bootstrap4.min.js",
              "src/assets/Template/vendor/datatables/jquery.dataTables.min.js",
              "src/assets/Template/js/sb-admin.min.js",
              "src/assets/Template/vendor/chart.js/Chart.bundle.min.js",
              "src/assets/Template/vendor/chart.js/Chart.min.js",
              "src/assets/Template/js/demo/chart-area-demo.js",
              "src/assets/Template/js/demo/chart-bar-demo.js",
              "src/assets/Template/js/demo/chart-pie-demo.js",
              "src/assets/Template/js/demo/datatables-demo.js"
            ],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "DynamiChart:build",
            "port": 2222
          },
          "configurations": {
            "production": {
              "browserTarget": "DynamiChart:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "DynamiChart:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "src/assets/Template/vendor/datatables/dataTables.bootstrap4.min.css",
              "src/assets/Template/css/sb-admin.min.css"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/popper.js/dist/umd/popper.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.min.js",
              "src/assets/Template/vendor/jquery/jquery.min.js",
              "src/assets/Template/vendor/jquery-easing/jquery.easing.min.js",
              "src/assets/Template/vendor/datatables/dataTables.bootstrap4.min.js",
              "src/assets/Template/vendor/datatables/jquery.dataTables.min.js",
              "src/assets/Template/js/sb-admin.min.js",
              "src/assets/Template/js/demo/chart-area-demo.js",
              "src/assets/Template/js/demo/chart-bar-demo.js",
              "src/assets/Template/js/demo/chart-pie-demo.js",
              "src/assets/Template/js/demo/datatables-demo.js"

            ],
            "assets": [
              "src/favicon.ico",
              {
                "glob": "**/*",
                "input": "src/assets/",
                "output": "/assets/"
              }
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "DynamiChart-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "DynamiChart:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "DynamiChart:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "DynamiChart"
}
